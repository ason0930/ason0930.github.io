{
  "name": "vue-easy-slider",
  "version": "5.2.7",
  "description": "vue-easy-slider is a slider component of Vue.js.",
  "keywords": [
    "slide",
    "swipe",
    "carrousel",
    "vue-swipe",
    "vue-swiper",
    "vue-slider",
    "vue-component"
  ],
  "homepage": "https://github.com/shhdgit/vue-easy-slider",
  "author": "Suhaha < jklopsdfw@gmail.com >",
  "repository": {
    "type": "git",
    "url": "git@github.com:shhdgit/vue-easy-slider.git"
  },
  "license": "MIT",
  "main": "dist/vue-easy-slider.umd.js",
  "module": "dist/vue-easy-slider.esm.js",
  "unpkg": "dist/vue-easy-slider.min.js",
  "jsdelivr": "dist/vue-easy-slider.min.js",
  "typings": "types/index.d.ts",
  "files": [
    "src",
    "dist/*.js",
    "types/*.d.ts"
  ],
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "cross-env NODE_ENV=production rollup --config build/rollup.config.browser.js",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.esm.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.umd.js",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.browser.js --watch",
    "test": "jest"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.26",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint-plugin-vue": "^4.7.1",
    "flush-promises": "^1.0.2",
    "jest": "^23.6.0",
    "rollup": "^0.67.3",
    "rollup-plugin-buble": "^0.19.4",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-eslint": "^5.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "stylus": "^0.54.5",
    "uglify-es": "^3.3.9",
    "vue": "^2.5.17",
    "vue-jest": "^3.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "dependencies": {}
}
